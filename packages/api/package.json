{
  "name": "@sudanflood/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./services/*": {
      "types": "./dist/services/*.d.ts",
      "import": "./dist/services/*.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.1000.0",
    "@aws-sdk/s3-request-presigner": "^3.1000.0",
    "@sudanflood/db": "workspace:*",
    "@sudanflood/shared": "workspace:*",
    "@trpc/server": "^11.0.0",
    "bcrypt": "^6.0.0",
    "drizzle-orm": "^0.39.3",
    "ioredis": "^5.10.0",
    "jose": "^6.1.3",
    "superjson": "^2.2.0",
    "xlsx": "^0.18.5",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^22.0.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  }
}
